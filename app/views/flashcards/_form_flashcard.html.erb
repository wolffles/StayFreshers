<%= form_for ([@flashcard.test_set,flashcard]) do |f| %>
  <div class="form-group">
    <%= f.label :question %>
      <%= f.text_field :question, class: 'form-control', placeholder: "Enter test_set name" %>

  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, rows: 8, class: 'form-control', placeholder: "Enter subject or description" %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
  <% end %>
