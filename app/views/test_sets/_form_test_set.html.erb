<%= form_for test_set do |f| %>
  <div class="form-group">
    <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', placeholder: "Enter test_set name" %>
  </div>
  <div class="form-group">
    <%= f.label :subject %>
    <%= f.text_area :subject, rows: 8, class: 'form-control', placeholder: "Enter subject or description" %>
  </div>

  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
  <% end %>
