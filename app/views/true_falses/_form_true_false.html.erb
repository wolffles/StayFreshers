<%= form_for ([@true_false.test_set,true_false]) do |f| %>
  <div class="form-group">
    <%= f.label :question %>
      <%= f.text_field :question, class: 'form-control', placeholder: "Enter test_set name" %>

  </div>
  <div class="form-group">
    <%= f.label :correct_answer, class: 'checkbox' do %>
      <%= f.check_box :correct_answer %> True or False
    <% end %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
  <% end %>
