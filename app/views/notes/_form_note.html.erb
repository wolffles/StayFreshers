<%= form_for note do |f| %>
  <div class="form-group">
    <%= f.label :subject %>
      <%= f.text_field :subject, class: 'form-control', placeholder: "Enter test_set subject" %>
  </div>
  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_area :body, rows: 16, class: 'form-control', placeholder: "Enter subject or description" %>
  </div>

  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
  <% end %>
